{
	"name": "reto-2",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"db:start": "docker compose up",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"docker:build": "docker build -t reto-2-plaiaundi .",
		"docker:run": "docker run -p 3000:3000 --env-file .env reto-2-plaiaundi",
		"docker:compose": "docker compose up -d"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.39.1",
		"@faker-js/faker": "^10.2.0",
		"@sveltejs/adapter-node": "^5.4.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/adapter-vercel": "^6.3.1",
		"@sveltejs/enhanced-img": "^0.9.3",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^24",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.6",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"dependencies": {
		"bootstrap": "5.2.3",
		"bootstrap-icons": "^1.13.1",
		"dotenv": "^17.2.3",
		"drizzle-kit": "1.0.0-beta.11-05230d9",
		"drizzle-orm": "1.0.0-beta.11-05230d9",
		"postgres": "^3.4.7",
		"valibot": "^1.2.0"
	}
}
